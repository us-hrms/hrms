<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"/>
	<!-- <constant name="struts.objectFactory.spring.autoWire" value="type" /> -->
    <package name="default" extends="struts-default">
    	<action name="*Order" class="orderAction" method="{1}">
    		<result type="redirect">${jspName}.jsp</result>
    	</action>
    	<action name="*Customer" class="customerAction" method="{1}" />
    	<action name="*Product" class="productAction" method="{1}" />
    	<action name="*OrderLine" class="orderLineAction" method="{1}" />
    </package>
</struts>
